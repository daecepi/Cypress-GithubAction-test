    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.7",
    "chalk": "^4.1.0",
    "critical": "^2.0.4",
    "css-loader": "^1.0.1",
    "cypress": "^6.3.0",
    "fancy-log": "^1.3.3",
    "gulp": ">=4",
