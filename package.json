{
  "name": "copper",
  "version": "1.0.0",
  "description": "<p align=\"center\"><a href=\"https://craftcms.com/\" target=\"_blank\"><img width=\"312\" height=\"90\" src=\"https://craftcms.com/craftcms.svg\" alt=\"Craft CMS\"></a></p>",
  "main": "gulpfile.js",
  "scripts": {
    "kss": "kss --config kss-config.json",
    "clean-folder": "rimraf web/js/vue",
    "clean-index": "rimraf web/js/vue/index.html",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run --browser chrome firefox",
    "build": "npm run clean-folder && webpack --mode production && npm run clean-index",
    "dev": "npm run clean-folder && webpack --mode development && npm run clean-index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProsperWorksDesign/copper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ProsperWorksDesign/copper/issues"
  },
  "paths": {
    "src": {
      "base": "./templates/",
      "css": "./src/css/",
      "fontello": "./src/fontello/",
      "fonts": "./src/fonts/",
      "json": "./src/json/",
      "js": "./src/js/",
      "img": "./src/img/",
      "scss": "./src/scss/"
    },
    "build": {
      "base": "./templates/",
      "css": "./web/css/",
      "fontello": "./web/fonts/fontello/",
      "fonts": "./web/fonts/",
      "js": "./web/js/",
      "html": "./web/html/",
      "img": "./web/imgs/"
    },
    "scss": [],
    "templates": "./templates/"
  },
  "urls": {
    "live": "https://copper.com",
    "local": "https://copper.lndo.site",
    "critical": "https://copper.lndo.site"
  },
  "vars": {
    "siteCssName": "style.css",
    "scssName": "style.scss",
    "cssName": "style.css"
  },
  "globs": {
    "critical": [
      {
        "url": "/",
        "template": "index"
      }
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "apollo-boost": "^0.4.7",
    "autoprefixer": "^7.1.1",
    "axios": "^0.18.1",
