    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.0.0",
    "eyeglass": "^1.6.0",
    "graphql": "^15.0.0",
    "gulp-minify": "^3.1.0",
    "imagemin-webp": "^6.0.0",
    "isotope-layout": "^3.0.4",
